{
  "name": "vue-i18n-ally",
  "displayName": "Vue i18n Ally",
  "description": "Better Vue i18n experiences with VS Code",
  "version": "0.10.11",
  "publisher": "antfu",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [
    "workspaceContains:package.json",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "keywords": [
    "vue",
    "vue-i18n",
    "vue i18n",
    "i18n"
  ],
  "icon": "static/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vue-i18n-ally"
  },
  "homepage": "https://github.com/antfu/vue-i18n-ally",
  "bugs": "https://github.com/antfu/vue-i18n-ally/issues",
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.vue-i18n-ally.config-locales",
        "title": "Vue i18n Ally: Manual configure locales path"
      },
      {
        "command": "extension.vue-i18n-ally.config-locales-auto",
        "title": "Vue i18n Ally: Auto detect locales path"
      },
      {
        "command": "extension.vue-i18n-ally.config-display-language",
        "title": "Vue i18n Ally: Change display language"
      },
      {
        "command": "extension.vue-i18n-ally.copy-key",
        "title": "Copy i18n key",
        "icon": "./static/icon-copy.svg"
      },
      {
        "command": "extension.vue-i18n-ally.translate-key",
        "title": "Translate",
        "icon": "./static/icon-translate.svg"
      },
      {
        "command": "extension.vue-i18n-ally.edit-key",
        "title": "Edit",
        "icon": "./static/icon-edit.svg"
      },
      {
        "command": "extension.vue-i18n-ally.open-key",
        "title": "Open locale file",
        "icon": "./static/icon-open-file.svg"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "extension.vue-i18n-ally.open-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /openable/",
          "group": "inline@1"
        },
        {
          "command": "extension.vue-i18n-ally.translate-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /translatable/",
          "group": "inline@2"
        },
        {
          "command": "extension.vue-i18n-ally.copy-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /node/",
          "group": "inline@3"
        },
        {
          "command": "extension.vue-i18n-ally.edit-key",
          "when": "view =~ /locales-tree/ && viewItem =~ /record/",
          "group": "inline@4"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vue-i18n-ally",
          "title": "Vue i18n Ally",
          "icon": "./static/logo-view-container.svg"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "locales-tree-file",
          "name": "Vue i18n Keys",
          "when": "vue-i18n-ally-enabled && vue-i18n-ally-supported-file"
        }
      ],
      "vue-i18n-ally": [
        {
          "id": "locales-progress",
          "name": "Progress",
          "when": "vue-i18n-ally-enabled"
        },
        {
          "id": "locales-tree",
          "name": "Tree",
          "when": "vue-i18n-ally-enabled"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vue i18n Ally",
      "properties": {
        "vue-i18n-ally.localesPaths": {
          "type": "string",
          "default": "",
          "description": "Path to locales directory (relative to project root)"
        },
        "vue-i18n-ally.sourceLanguage": {
          "type": "string",
          "default": "en",
          "description": "Source language for machine translation"
        },
        "vue-i18n-ally.displayLanguage": {
          "type": "string",
          "default": "en",
          "description": "Default display language"
        }
      }
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "‚ö° Features"
      },
      {
        "type": "fix",
        "section": "üêû Bug Fixes"
      },
      {
        "type": "refactor",
        "section": "üîÆ Refactor"
      },
      {
        "type": "test",
        "section": "‚öó Tests"
      },
      {
        "type": "docs",
        "section": "üìö Documentations"
      },
      {
        "type": "ci",
        "hidden": true
      }
    ]
  },
  "scripts": {
    "release": "standard-version",
    "publish": "standard-version && vsce publish",
    "vscode:prepublish": "webpack --mode development",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.133",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "rimraf": "2.6.3",
    "standard-version": "6.0.1",
    "ts-loader": "6.0.2",
    "typescript": "3.5.1",
    "vscode": "1.1.34",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2"
  },
  "dependencies": {
    "fast-glob": "^2.2.7",
    "flat": "^4.1.0",
    "js-yaml": "^3.13.1",
    "limax": "^1.7.0",
    "lodash": "^4.17.11",
    "translation.js": "^0.7.8"
  }
}
